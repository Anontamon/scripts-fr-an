<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>vnpackage@openroad.net.au</em:id>
    <em:version>1.0</em:version>

    <!-- nsIUpdateItem type for a Multiple Item Package -->
    <em:type>32</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>true</em:unpack>

    <!-- Front End MetaData -->
    <em:name>VN Font Package</em:name>
    <em:description>Provides font fall back for Tai Viet and Cham scripts. Also provides menu toggle for Firefox graphite support that is required by the Tai Viet font.</em:description>
    <em:creator>Andj</em:creator>

    <!-- Firefox Android version --> 
    <em:targetApplication>
      <Description>
        <em:id>{aa3c5121-dab2-40e2-81ca-7ea25febc110}</em:id>
        <em:minVersion>19.0a1</em:minVersion>
        <em:maxVersion>24.0</em:maxVersion>
      </Description>
    </em:targetApplication>
    
  </Description>
</RDF>
    